package org.vertigo.domain;


public class SampleDTO {
	
	int tno;
	String title;
	String regdate;
	
	// 생성자 주입
	public SampleDTO (int tno,String title , String reg) {
		this.tno = tno;
		this.title = title;
		this.regdate = reg;
	}

	@Override
	public String toString() {
		return "SampleDTO [tno=" + tno + ", title=" + title + ", regdate=" + regdate + "]";
	}
	
	
	
}
